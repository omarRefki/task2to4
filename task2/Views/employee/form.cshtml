@model List<employee>
<form action="/employee/addnew" class="form-control">

    <table>
        <tr>
            <th> <label for="first-name">First Name:</label></th>
            <th><input type="text" id="first-name" name="Fname"class="form-control"></th>
        </tr>
        <tr>
            <th><label for="last-name">Last Name:</label></th>
            <th><input type="text" id="last-name" name="Lname" class="form-control"></th>
        </tr>
        <tr>
            <th> <label for="ssn">SSN:</label></th>
            <th><input type="text" id="ssn" name="SSN" class="form-control"></th>
        </tr>
        <tr>
            <th><label for="salary">Salary:</label></th>
            <th> <input type="text" id="salary" name="salary" class="form-control"></th>
        </tr>
        <tr>
            <th> <label for="address">Address:</label></th>
            <th> <input type="text" id="address" name="address" class="form-control"><br></th>
        </tr>
        <tr>
            <th>
                <label for="super">SuperVisor</label>
            </th>
            <th>

                <select name="superid" id="super">
                    <option value="">select supervisor</option>
                    @foreach (var item in Model)
                    {
                        <option value="@item.SSN">@item.Fname @item.Lname</option>
                    }
                </select>

            </th>
        </tr>

    </table>


    <input type="submit" value="Submit" class="btn btn-success">
</form>